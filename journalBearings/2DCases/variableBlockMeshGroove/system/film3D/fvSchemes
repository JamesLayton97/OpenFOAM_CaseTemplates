FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/TEHL";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         steadyState;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         			Gauss linear;
    div(interpolate((1-g)*U*h),relRho) 	bounded Gauss upwind; 
    div(interpolate(g*U*h),relRho) 	Gauss linear; 
    div(interpolate(U),T)		bounded Gauss upwind;
}

laplacianSchemes
{
    default         Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}

// ************************************************************************* //
