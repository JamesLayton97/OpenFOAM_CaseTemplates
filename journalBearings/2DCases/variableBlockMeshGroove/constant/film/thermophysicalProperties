FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/fluid";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{

    specie
    {
        molWeight       18;
    }
    equationOfState
    {
        rho             860;
    }
    thermodynamics
    {
        Cp              2000;
        Hf              0;
    }
    transport
    {
        mu              0.02;
        Pr              300;        // Pr = nu*cp/k
    }
}

