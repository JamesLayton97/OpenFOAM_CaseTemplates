FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/fluid";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

mixture
{
    specie
    {
            molWeight   30;
    }
    equationOfState
    {
	    rho		870;
    }
    thermodynamics
    {
        Cp          2000;
        Hf          0;
    }
    transport
    {
        mu          0.0293;
        Pr          450;
    }
}



phases (oil air);

pMin 	1000;

sigma 	0;

